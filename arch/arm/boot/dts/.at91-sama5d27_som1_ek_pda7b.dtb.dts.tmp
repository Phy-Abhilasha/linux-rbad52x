    atmel,can-isoc;
   };

   ep@2 {
    reg = <2>;
    atmel,fifo-size = <1024>;
    atmel,nb-banks = <3>;
    atmel,can-dma;
    atmel,can-isoc;
   };

   ep@3 {
    reg = <3>;
    atmel,fifo-size = <1024>;
    atmel,nb-banks = <2>;
    atmel,can-dma;
    atmel,can-isoc;
   };

   ep@4 {
    reg = <4>;
    atmel,fifo-size = <1024>;
    atmel,nb-banks = <2>;
    atmel,can-dma;
    atmel,can-isoc;
   };

   ep@5 {
    reg = <5>;
    atmel,fifo-size = <1024>;
    atmel,nb-banks = <2>;
